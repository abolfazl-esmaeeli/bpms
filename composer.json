{
    "name": "niyam/bpms",
    "description": "A simple BPMN Library.",
    "keywords": ["bpms", "niyam", "workflow"],
    "license": "MIT",
    "authors": [{
        "name": "David Dashti",
        "email": "mddashti@gmail.com"
    }],
    "require": {
        "php": ">=5.4",
        "illuminate/database": "~5.4",
        "nesbot/carbon": "~1.22",
        "symfony/dom-crawler": "^3.3",
        "symfony/expression-language": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "Niyam\\Bpms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niyam\\Bpms\\BpmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}